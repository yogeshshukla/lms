<?php

function mymodule_preprocess_page(&$variables) {
  $variables['#attached']['library'][] =  'mymodule/mymodule-lib';
}
/**
 * Implements hook_preprocess_links().
 */
function mymodule_preprocess_links(&$variables) {
  
    if (isset($variables['field-content'])) {
      $variables['field-content']['link']['#title'] = t('Unsubscribe');
    }
  
}

?>
